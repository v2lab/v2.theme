.image-container
{
    margin-left:6px;
}

.album_image
{
    float: left;
    width:108px;
    height:108px;
    margin-right:3px;
    margin-bottom:3px;
    overflow:hidden;
    border:1px solid #999999;
    position:relative;
}

.album_image .thumbnail
{
    position:static;
    margin:0;
    top:0;
    left:0;
    display:none;
    opacity:1;
}

.album_image img
{
    position:absolute;
    left:50%;
    top:50%;
    margin-left:-13px;
    margin-top:-13px;
    opacity:0.5;
}

.album_image img:hover
{
    opacity:1;
}

.nav_spacer
{
    clear:both;
    margin-bottom:6px;
}

.albumWrapper
{
    margin-left:6px;
}

.image_navigation
{
    position:relative;
    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    margin-left: -3px;
    text-align: center;
}

.media_navigation
{
    height:30px;
}

.media_navigation a.image-next-button
{
    background:transparent url(right_arrow_big.png) no-repeat scroll right top !important;
}

.media_navigation a.image-prev-button
{
    background:transparent url(left_arrow_big.png) no-repeat scroll left top !important;
}

.image_navigation:hover a.image-next-button, .image_navigation:hover a.image-prev-button
{
    background:transparent url(right_arrow_big.png) no-repeat scroll right top !important;
}

.image_navigation:hover a.image-prev-button
{
    background:transparent url(left_arrow_big.png) no-repeat scroll left top !important;
}

a.image-next-button:hover, a.image-prev-button:hover
{
    opacity:1;
}


a.image-next-button, a.image-prev-button
{
    position:absolute;
    cursor:pointer;
    height: 99%;
    width:70px;
    z-index: 10;
    opacity:0.5;
    margin: 0px 8px;
    background:none;
}

a.image-next-button
{
    top: 0;
    right: 6px;
}

a.image-prev-button
{
    top: 0;
    left: 6px;
}

.expandedView
{
    width:762px;
}

.expandedView #parent-fieldname-text
{
}

.expanded-button
{
    border-top:1px solid #cccccc;
    border-right:1px solid #cccccc;
    margin:0px 6px;
