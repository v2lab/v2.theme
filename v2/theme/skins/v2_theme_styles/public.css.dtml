}

div.result-leadimage
{
    position: relative;
}

div.result-leadimage img
{
    position:absolute;
    margin-top:-13px;
    margin-left:-13px;
    top: 50%;
    left: 50%;
    opacity:0.5;
}

div.result-leadimage:hover img
{
    opacity: 1;
}

div.result-leadimage img.leadimage
{
    max-height:100px;
    max-width:125px;
