    opacity:1;
}

div.result-leadimage
{
    float:right;
}


/* End edit form elements */

#visual-portal-wrapper {
background-image:url(&dtml-menuShadowImage;);
background-repeat: repeat-x;
margin-right: auto;
margin-left: auto;
padding: 0;
width: 1050px;
/*max-width:1200px;
min-width:1050px;*/
border-left:solid 1px #f0f0f0;
border-right:solid 1px #f0f0f0;
border-bottom:solid 1px #f0f0f0;
}

/* Logo properties */
#portal-logo {
border: 0;
padding: 0;
margin: 0;
float: left;
}

/* The skin switcher at the top, only shows up if you have multiple skins available */
#portal-skinswitcher {
}
#portal-skinswitcher a {
display: block;
float: left;
}
#portal-top {
/* Top section */
margin: 0;
padding: 0;
background-color: transparent;
}

/* Site-wide action menu - font size, contact, index, sitemap etc */
#portal-siteactions {
background-color: transparent;
white-space: nowrap;
list-style-image: none;
list-style-type: none;
margin: 1px 10px;
height: auto;
line-height: normal;
float: right;
padding: 3px 2em 3px 0;
}

#portal-siteactions li {
display: inline;
}
#portal-siteactions li a {
background-color: transparent;
color: &dtml-discreetColor;;
height: auto;
padding: 0.2em;
margin-right: 0.2em;
text-decoration: none;
text-transform: &dtml-textTransform;;
}
#portal-siteactions li.selected a {
background-color: &dtml-globalBackgroundColor;;
color: &dtml-globalFontColor;;
border: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
}
#portal-siteactions li a:hover {
color: &dtml-globalFontColor;;
}


/* Searchbox style and positioning */

#portal-searchbox {
float: right;
background-color: transparent;
margin: 1em 0em 0em 0em;
padding: 0 2em 0 0;
text-align: right;
text-transform: &dtml-textTransform;;
white-space: nowrap;
z-index: 2;
}
#portal-advanced-search {
margin-top: 0.2em;
clear: both;
}
#portal-advanced-search a {
color: &dtml-discreetColor;;
text-decoration: none;
text-transform: &dtml-textTransform;;
}

/* Search results elements */

dl.searchResults dt {
font-size: 140%;
font-weight: normal;
}

form.searchPage {
text-align: center;
}

input.searchPage {
font-size: 200% !important;
width:343px;
}

form.searchPage input.searchButton {
background-position:5px 7px;
padding:1px 10px 1px 25px;
width:auto;
}

/* LiveSearch styles */
.LSRes {
font-family: <dtml-var fontFamily>;
visibility: visible;
color: &dtml-globalFontColor;;
background-color: &dtml-backgroundColor;;
vertical-align: middle;
display:block;
list-style-image: none;
list-style-type: none;
text-align: left;
min-width: 16.5em;
text-transform: none;
margin-left: 0;
line-height: 1.1em;
}
#LSHighlight,
.LSHighlight {
background-color: &dtml-globalBackgroundColor;;
border: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
color: &dtml-globalFontColor;;
}
.LSTable {
margin: 1em 0;
}
.LSRow {
border: &dtml-borderWidth; &dtml-borderStyle; &dtml-backgroundColor;;
white-space: normal;
padding:0;
margin: 0;
list-style-image: none;
list-style-type: none;
}
.LSRow a {
text-decoration: none;
font-weight:bold;
white-space:nowrap
}
.LSDescr {
color: &dtml-discreetColor;;
text-transform: &dtml-textTransform;;
padding-left:2.1em;
margin-top:-0.1em;
}
.LSResult {
position: relative;
display: block;
text-align: right;
padding-top: 5px;
margin: 0;
left: 3px;
z-index: 3;
}
.LSShadow {
position: relative;
text-align: right;
}
.livesearchContainer {
background-color: &dtml-backgroundColor;;
margin-top: 0;
padding: 0 !important;
position: absolute;
right: 0px;
/* Currently exposes a rendering bug in Mozilla */
top: 0;
white-space: normal;
font-family: <dtml-var fontFamily>;
visibility: visible;
text-align: left;
color: &dtml-globalFontColor;;
border: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
width: 30em;
text-transform: none;
}
* html .livesearchContainer {
padding: &dtml-borderWidth; !important;
padding-top: 0 !important;
background-color: &dtml-globalBorderColor;;
border: 0;
}
#livesearchLegend {
line-height: 1em;
margin-top: -2em;
margin-left: -0.1em;
border: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;;
border-bottom: 0;
}
* html #livesearchLegend {
margin-top: -1.9em;
margin-left: -8px;
position: relative;
}

/* Workaround for Internet Explorer's broken z-index implementation */
.LSIEFix {
background-color: &dtml-backgroundColor;;
padding: 0.5em !important;
z-index: 20;
}
.LSBox {
clear: left;
float: left;
text-align: right;
padding-right: 1px;
display:block;
}
#LSNothingFound {
text-align: center;
padding: 2px;
}
.LSBox label {
font-weight: normal;
}

/* ------------------------ V2 GLOBAL SECTIONS---------------------------------- */

#portal-globalnav {
    background-color:transparent;
    background-position: bottom right;
    background-repeat:no-repeat;
    float:left;
    height:auto;
    line-height:normal;
    list-style-image:none;
    list-style-position:outside;
    list-style-type:none;
    margin:0px;
    padding:0;
    white-space:nowrap;
    position:relative;
    z-index:3;
}

#portal-globalnav ul {
    margin:6px;
}

#portal-globalnav li {
    float:left;
    display:inline;
    margin-top:6px;
    position:relative;
    padding-top:0.5em;
    padding-left:0.5em;
    padding-bottom:8px;
    padding-right:0.5em;
}

#portal-globalnav li a.selected {
    color: black;
}

#portal-globalnav li.selected a {
    color: black;
}

#portal-globalnav li a {
    background-color:transparent;
    color:&dtml-globalStrongColor;;
    font-size:&dtml-fontMenuSize;;
    height:auto;
    margin:0;
    padding-bottom:1px;
    padding-left:1.25em;
    padding-right:1.25em;
    text-decoration:none;
    text-transform:capitalize;
    white-space: nowrap;
}

#portal-globalnav .submenu li a {
    color:#ffffff;
    padding-left: 1.5em;
}

#portal-globalnav li a:hover {
    background-color:#ffffff;
    color: &dtml-globalStrongColor;;
    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
}

#portal-globalnav li:hover {
    border:medium none;
    color:#ffffff;
    background-color:&dtml-globalStrongColor;;
    text-decoration:none;
    opacity:0.9;
    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
}

#portal-globalnav li:hover a {
    color:#ffffff;
}

#portal-globalnav li:hover a:hover {
    color:&dtml-globalStrongColor;;
}

#portal-globalnav .submenu
{
    background-color:&dtml-globalStrongColor;;
    font-weight:normal;
    position:absolute;
    color:#ffffff;
    left:0;
    /*top:24px;*/
    top:2.5em;
    font-size:80%;
    margin:0;
    padding:0;
    display:none;
    z-index: 3;
    width:auto;
    min-width:100%;
    border-radius-topright:5px;
    border-radius-bottomleft:5px;
    border-radius-bottomright:5px;
    -moz-border-radius-topright:5px;
    -moz-border-radius-bottomleft:5px;
    -moz-border-radius-bottomright:5px;
    -webkit-border-top-right-radius:5px;
    -webkit-border-bottom-left-radius:5px;
    -webkit-border-bottom-right-radius:5px;
}

#portal-globalnav .submenu li:hover
{
    background-color:#ffffff;
    color: &dtml-globalStrongColor;;
    border-radius-topleft:5px;
    border-radius-topright:5px;
    border-radius-bottomleft:5px;
    border-radius-bottomright:5px;
    -moz-border-radius-topleft:5px;
    -moz-border-radius-topright:5px;
    -moz-border-radius-bottomleft:5px;
    -moz-border-radius-bottomright:5px;
    -webkit-border-top-left-radius:5px;
    -webkit-border-top-right-radius:5px;
    -webkit-border-bottom-left-radius:5px;
    -webkit-border-bottom-right-radius:5px;
}

#portal-globalnav .submenu li:hover a
{
    color:&dtml-globalStrongColor;;
}


#portal-globalnav .submenu li a:hover
{
    background-color:transparent;
    color:&dtml-globalStrongColor;;
}

#portal-globalnav li:hover .submenu
{
    display:block;
}

#portal-globalnav li ul:hover
{
    display:block;
}

#portal-globalnav .submenu li
{
    display:block;
    float:none;
    margin-top:0.5em;
    padding-top:0.2em;
    padding-bottom:0.2em;
    margin-left:3px;
    margin-right:3px;
}

