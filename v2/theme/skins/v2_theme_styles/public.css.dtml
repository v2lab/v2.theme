}

div.search_result_box
{
    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    border: 1px solid #dcdcdc;
    background-color: transparent;
    background-image:url(&dtml-backgroundOpacity;);
    margin:6px 6px;
    padding:6px;
    float: left;
    width: 47.4%;
    height: 100px;
    max-height: 100px;
}

div.resultTypes {
    text-align: center;
    margin-top: 0;
}

div.resultTypes ul
{
    list-type:none;
    list-style:none;
    margin-left:36px;
}

div.resultTypes ul li
{
    display:inline;
    margin-right:37px;

}

div.resultTypes ul li a, div.resultTypes ul li a:visited, div.resultTypes ul li a:active
{
