}

.tileItem .autoFlowPlayer:hover img
{
    opacity:1;
}

.tileItem .autoFlowPlayer img.splash
{
    position:static;
    margin:0px;
    opacity:1;
    height:192px;
    width:346px;
}

.file-view .audio
{
    width:774px !important;
